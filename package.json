{
  "name": "flutter_extension",
  "version": "0.1.1",
  "description": "Generates Flutter classes from colors, text styles and text layers code",
  "scripts": {
    "start": "zem start",
    "build": "zem build",
    "clean": "zem clean",
    "exec": "zem exec"
  },
  "zeplin": {
    "displayName": "Flutter",
    "projectTypes": [
      "android",
      "ios"
    ],
    "options": [
      {
        "name": "Color/Style class name Prefix",
        "type": "text",
        "id": "classPrefix",
        "default": ""
      }
    ]

  },
  "dependencies": {
    "zem": "^0.1.2"
  }
}
